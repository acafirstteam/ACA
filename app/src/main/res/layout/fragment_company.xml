<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android = "http://schemas.android.com/apk/res/android"
	xmlns:app = "http://schemas.android.com/apk/res-auto"
	android:layout_width = "match_parent"
	android:layout_height = "match_parent"
	>

	<ImageView
		android:id = "@+id/logo"
		android:layout_width = "64dp"
		android:layout_height = "64dp"
		android:layout_marginTop="30dp"
		android:contentDescription = "@string/todo"
		app:layout_constraintLeft_toLeftOf = "parent"
		app:layout_constraintRight_toRightOf = "parent"
		app:layout_constraintTop_toTopOf = "parent" />

	<ImageView
		android:id = "@+id/partner1"
		android:layout_width = "40sp"
		android:layout_height = "40sp"
		android:layout_marginBottom="30dp"
		android:contentDescription = "@string/todo"
		android:tag="http://volozhka.org"
		app:layout_constraintBottom_toBottomOf = "parent"
		app:layout_constraintLeft_toLeftOf = "parent"
		app:layout_constraintRight_toLeftOf = "@+id/partner2" />


	<ImageView
		android:id = "@+id/partner2"
		android:layout_width = "40sp"
		android:layout_height = "40sp"
		android:layout_marginBottom="30dp"
		android:contentDescription = "@string/todo"
		android:tag="http://www.kuhmaster.com"
		app:layout_constraintBottom_toBottomOf = "parent"
		app:layout_constraintLeft_toRightOf = "@id/partner1"
		app:layout_constraintRight_toLeftOf = "@+id/partner3" />

	<ImageView
		android:id = "@+id/partner3"
		android:layout_width = "40sp"
		android:layout_height = "40sp"
		android:layout_marginBottom="30dp"
		android:contentDescription = "@string/todo"
		android:tag="http://hlebspas.ru"
		app:layout_constraintBottom_toBottomOf = "parent"
		app:layout_constraintLeft_toRightOf = "@id/partner2"
		app:layout_constraintRight_toLeftOf = "@+id/partner4" />

	<ImageView
		android:id = "@+id/partner4"
		android:layout_width = "40sp"
		android:layout_height = "40sp"
		android:layout_marginBottom="30dp"
		android:contentDescription = "@string/todo"
		android:tag="http://sladoyar.ru"
		app:layout_constraintBottom_toBottomOf = "parent"
		app:layout_constraintLeft_toRightOf = "@id/partner3"
		app:layout_constraintRight_toRightOf = "parent" />

	<TextView
		android:id = "@+id/partners"
		android:layout_width = "match_parent"
		android:layout_height = "wrap_content"
		android:padding = "10dp"
		android:layout_marginBottom="30dp"
		android:textAlignment = "center"
		android:textSize = "18sp"
		android:textStyle = "italic"
		android:typeface = "serif"
		app:layout_constraintEnd_toEndOf = "parent"
		app:layout_constraintStart_toStartOf = "parent"
		app:layout_constraintBottom_toTopOf="@id/partner1"/>


	<TextView
		android:id = "@+id/text"
		android:layout_width = "match_parent"
		android:layout_height = "wrap_content"
		android:padding = "10dp"
		android:layout_marginTop="30dp"
		android:textAlignment = "center"
		android:textSize = "18sp"
		android:textStyle = "italic"
		android:typeface = "serif"
		app:layout_constraintEnd_toEndOf = "parent"
		app:layout_constraintStart_toStartOf = "parent"
		app:layout_constraintTop_toBottomOf = "@id/logo" />

</androidx.constraintlayout.widget.ConstraintLayout>